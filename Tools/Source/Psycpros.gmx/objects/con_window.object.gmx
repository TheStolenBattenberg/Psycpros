    gui_menu_add_element(mFile, gui_element_define(Element.Button, "Open Configuration"));
    gui_menu_add_element(mFile, gui_element_define(Element.Button, "Open Console"));
    gui_menu_add_element(mFile, gui_element_define(Element.Break));
    gui_menu_add_element(mFile, gui_element_define(Element.Button, "Exit"));
}

//Create the Import Menu
mImport = gui_menu_create(" Import ", false); {
    //Create 2D menu
    ms2D = gui_menu_create("2D", true); {
        gui_menu_add_element(ms2D, gui_element_define(Element.Button, "Playstation Texture (.TIM)"));
        gui_menu_add_element(ms2D, gui_element_define(Element.Button, "FromSoft Texture (.RTIM)"));
    }
    
    //Create 3D menu
    ms3D = gui_menu_create("3D", true); {
        gui_menu_add_element(ms3D, gui_element_define(Element.Button, "Playstation TMD (.TMD)"));
        gui_menu_add_element(ms3D, gui_element_define(Element.Button, "FromSoft AnimTMD (.MO)"));
        
    }

    gui_menu_add_element(mImport, gui_element_define(Element.Menu, "2D", ms2D));
    gui_menu_add_element(mImport, gui_element_define(Element.Menu, "3D", ms3D));
}

//Create Menubar using a list.
wMenubar = ds_list_create(); {
    ds_list_add(wMenubar, mFile);
    ds_list_add(wMenubar, mImport);
}
